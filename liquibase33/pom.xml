<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
		
	<changeSet id="1" author="bob">
		<createTable tableName="tabel-liquibase">
			<column name="NIS" type="int">
				<constraints primaryKey="true" nullable="true"/>
			</column>
			<column name="nama" type="varchar(50)">
				<constraints nullable="true"/>
			</column>
			<column name="kelas" type="varchar(30)">
				<constraints nullable="true"/>
			</column>
			<column name="jurusan" type="varchar(30)">
				<constraints nullable="true"/>
			</column>
			<column name="alamat" type="varchar(100)">
				<constraints nullable="true"/>
			</column>
		</createTable>
	</changeSet>

</databaseChangeLog> 